M(101) => b = 3
M(102) => d = 5
M(200) => c = 0 (stor destination)

if( a>=b )
    	c = a-b;
else 
	c = a+b;

load M(100) , sub M(101)
conditional jmp( a >= b ) , NULL        // jump to the if() instructions

-- else set --							
load M[100] , add M[101]				
stor M[200], NULL
HALT()

-- if set --
load M[100] , sub M[101]                => this implemented here i.e a conditional jump and sub
